<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

    <!-- Servlets -->
    <servlet>
        <servlet-name>servlet</servlet-name>
        <servlet-class>hatanian.david.gwt.server.DemoServiceImpl</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>servlet</servlet-name>
        <url-pattern>/appenginedemo/demo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>demo</servlet-name>
        <servlet-class>hatanian.david.gwt.server.DemoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>demo</servlet-name>
        <url-pattern>/demoservlet</url-pattern>
    </servlet-mapping>

    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>AppEngineDemo.html</welcome-file>
    </welcome-file-list>
</web-app>
